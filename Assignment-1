#include <stdio.h>

int main() {
    int rows = 3;
    int cols = 4;
    int start = 1;
    int step = 2;
    
    for (int i = 0; i < rows; i++) {
        for (int j = 0; j < cols; j++) {
            printf("%d ", start + j * step);
        }
        start += step * cols;
        printf("\n");
    }
    
    return 0;
}
